		0px 3px 18px 0px var(--color-base-00);
	--shadow-strong: 
		0px 0px 24px 0px var(--trans-dark-00);
	
	--glass-base: 
		inset 1.125px 1.75px 2px -1.25px rgba(255, 255, 255, 0.25), 
		inset -1.125px -1.75px 2px -1.25px rgba(255, 255, 255, 0.175), 
		inset 0 8px 16px rgba(255, 255, 255, 0.025);	
	--glass-base-m: 
		inset 1.125px 1.75px 2px -1.25px rgba(255, 255, 255, 0.25), 
		inset -1.125px -1.75px 2px -1.25px rgba(255, 255, 255, 0.175), 
		inset 0 8px 16px rgba(255, 255, 255, 0.025);
	--glass-base-l: 
		inset 1.125px 1.75px 3px -1.25px rgba(255, 255, 255, 0.15), 
		inset -1.125px -1.75px 3px -1.25px rgba(255, 255, 255, 0.1), 
		inset 0 8px 16px rgba(255, 255, 255, 0.015);
	--glass-button: 
		inset 2px 2px 2.5px -0.5px oklab(from var(--interactive-accent) calc(l + 0.15) a b / 0.75), 
		inset -2px -2px 2.5px -0.5px oklab(from var(--interactive-accent) calc(l + 0.1) a b / 0.75);
	--glass-button-l: 
		inset 3px 3px 3px -1px oklab(from var(--interactive-accent) calc(l + 0.15) a b / 0.75), 
		inset -3px -3px 3px -1px oklab(from var(--interactive-accent) calc(l + 0.1) a b / 0.75);

	--shadow-s: 
		var(--glass-base),
		var(--shadow-normal);
	--shadow-l: 
		var(--glass-base-l),
		var(--shadow-strong);

	/* Interface shadows */

	--shadow-input: 
		0px 0px 0px 0.5px var(--trans-faint-30);
	--shadow-input-active: 
		0 0 0 2.25px var(--color-accent-4);
	--shadow-muted: 
		0px 8px 16px 0px var(--trans-strong-10);	


	--bases-cards-shadow-hover: 
		0 0 0 0.375px var(--trans-faint-60);
	--shadow-workspace: 
		0 -0.25px 1.75px -1px var(--trans-faint-60),
		0 0 16px var(--trans-strong-00);
	--shadow-workspace-multi: none;
	--shadow-fab: 
		inset 0 1.25px 2px 0px var(--trans-faint-10),
		inset 0 -1px 2.5px 0px var(--trans-strong-00),
		0 3px 12px -3px var(--trans-dark-10);
	--shadow-fab-active: 
		inset 0 1.25px 2px 0px var(--color-accent-5),
		inset 0 1.25px 2px 0px var(--trans-faint-05),
		inset 0 -1px 2.5px 0px var(--trans-strong-00),
		0 5px 15px -1.5px var(--color-accent-5),
		0 5px 15px -3px var(--trans-dark-00);
	--shadow-sidebar: 
		0px 0px 12px 0px var(--trans-strong-20);
	--shadow-tab:
		var(--glass-base);

	--menu-shadow: var(--shadow-normal);

	/* Component shadows */
	--shadow-button: 
		var(--glass-button), 
		var(--shadow-muted);
	--shadow-input: 
		0 0 0 0.5px var(--trans-faint-40);
	--shadow-input-active: 
		0 0 0 2.5px var(--color-accent-4);
	--shadow-toggle:
		inset 1.25px 1.25px 2px 0px oklab(from var(--interactive-accent) calc(l + 0.125) a b / 0.75), 
		inset -1.25px -1.25px 2px 0px oklab(from var(--interactive-accent) calc(l + 0.075) a b / 0.75);
	/* Markdown item shadows */

	/* Modal interface shadows */
	--shadow-setting-items: 
		0 4px 12px 0 var(--trans-strong-10);
	--shadow-setting-items-top: 
		inset 1px 2px 2px 0 var(--trans-strong-10),
		0px 1px 9px var(--trans-faint-05);
	--shadow-suggestion-item: 
		var(--glass-button-l),
		var(--shadow-muted);
	
	/* Other shadows */
	--bases-cards-shadow-hover: 
		0 0.125px 0px 0.375px var(--trans-faint-60);	
}
